import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;


public class DataAnalysis {
	public void reader(Scanner input) {
		String line = input.nextLine();
		int count = 0;
		for (int i=0; i<line.length(); i++)
			count++;
		count = count/2;
		System.out.println(count);
	}
}
